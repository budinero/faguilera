@article{mazzoletti_model-based_2017,
  title = {A {{Model-Based Strategy}} for {{Interturn Short-Circuit Fault Diagnosis}} in {{PMSM}}},
  author = {Mazzoletti, M. A. and Bossio, G. R. and Angelo, C. H. De and {Espinoza-Trejo}, D. R.},
