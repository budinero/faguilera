@article{aguilera_current-sensor_2016,
  title = {Current-sensor fault detection and isolation for induction-motor drives using a geometric approach},
  author = {Aguilera, F. and {\noopsort{barrera}}{de la Barrera}, P. M. and De Angelo, C. H. and Espinoza Trejo, D. R.},
  year = {2016},
  month = aug,
  journal = {Control Engineering Practice},
  volume = {53},
  pages = {35--46},
  issn = {0967-0661},
  doi = {10.1016/j.conengprac.2016.04.014},
  urldate = {2016-05-11},
  abstract = {This work presents the design of a current-sensor fault detection and isolation system for induction-motor drives. A differential geometric approach is addressed to determine if faults can be detected and isolated in drives with two line current sensors by using a model based strategy. A set of subsystems is obtained based on the observability co-distribution, whose outputs are decoupled from the load torque (detectability) and only affected by one of the sensors (isolability). A bank of observers is designed for these subsystems in order to obtain residuals for the fault detection and isolation. It is demonstrated that the proposed strategy allows detecting single and multiple sensor faults, including disconnection, offset and gain faults. Experimental results validate the proposal.},
  copyright = {All rights reserved},
  keywords = {citar},
  file = {/home/facu/documentos/GEA/papers/2016/Aguilera et al_2016_Current-sensor fault detection and isolation for induction-motor drives using a.pdf;/home/facu/documentos/datos/Zotero/storage/DCGURPNA/S0967066116300843.html}
}

