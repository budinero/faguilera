@article{vogelsberger_using_2010,
  title = {Using {{PWM-Induced Transient Excitation}} and {{Advanced Signal Processing}} for {{Zero-Speed Sensorless Control}} of {{AC Machines}}},
  author = {Vogelsberger, M.A. and Grubic, S. and Habetler, T.G. and Wolbank, T.M.},
  year = {2010},
