@article{adnan_load_2019,
  title = {Load flow balancing and transient stability analysis in renewable integrated power grids},
  author = {Adnan, Muhammad and Tariq, Muhammad and Zhou, Zhenyu and Poor, H. Vincent},
  year = {2019},
  month = jan,
  journal = {International Journal of Electrical Power \& Energy Systems},
  volume = {104},
