@article{thirugnanam_energy_2021,
  title = {Energy {{Management}} of {{Grid Interconnected Multi-Microgrids Based}} on {{P2P Energy Exchange}}: {{A Data Driven Approach}}},
  shorttitle = {Energy {{Management}} of {{Grid Interconnected Multi-Microgrids Based}} on {{P2P Energy Exchange}}},
  author = {Thirugnanam, Kannan and Moursi, Mohamed Shawky El and Khadkikar, Vinod and Zeineldin, Hatem H. and Al Hosani, Mohamed},
