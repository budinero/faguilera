  ids = {salvadori_smart_2013-1},
  title = {Smart {{Grid Infrastructure Using}} a {{Hybrid Network Architecture}}},
  author = {Salvadori, F. and Gehrke, C. S. and {\noopsort{oliveira}}{de Oliveira}, A. C. and {\noopsort{campos}}{de Campos}, M. and Sausen, P. S.},
