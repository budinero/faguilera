@article{zine_interests_2018,
  title = {Interests and {{Limits}} of {{Machine Learning-Based Neural Networks}} for {{Rotor Position Estimation}} in {{EV Traction Drives}}},
  author = {Zine, W. and Makni, Z. and Monmasson, E. and Idkhajine, L. and Condamin, B.},
