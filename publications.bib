@article{sathiyan_comprehensive_2022,
  title = {Comprehensive {{Assessment}} of {{Electric Vehicle Development}}, {{Deployment}}, and {{Policy Initiatives}} to {{Reduce GHG Emissions}}: {{Opportunities}} and {{Challenges}}},
