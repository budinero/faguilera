@article{garramiola_hybrid_2020,
  title = {A {{Hybrid Sensor Fault Diagnosis}} for {{Maintenance}} in {{Railway Traction Drives}}},
  author = {Garramiola, Fernando and Poza, Javier and Madina, Patxi and {\noopsort{olmo}}{del Olmo}, Jon and Ugalde, Gaizka},
  year = {2020},
