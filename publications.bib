@article{meinguet_method_2013,
  title = {A {{Method}} for {{Fault Detection}} and {{Isolation Based}} on the {{Processing}} of {{Multiple Diagnostic Indices}}: {{Application}} to {{Inverter Faults}} in {{AC Drives}}},
