@article{zia_microgrids_2018,
  title = {Microgrids energy management systems: {{A}} critical review on methods, solutions, and prospects},
  shorttitle = {Microgrids energy management systems},
  author = {Zia, Muhammad Fahad and Elbouchikhi, Elhoussin and Benbouzid, Mohamed},
