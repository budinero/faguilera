  langid = {english},
  file = {/home/facu/documentos/GEA/papers/2019/Korzonek et al_2019_A review on MRAS-type speed estimators for reliable and efficient induction.pdf;/home/facu/documentos/datos/Zotero/storage/XA3FNCN4/S0019057819301533.html}
