@inproceedings{aguilera_fault_2015,
  title = {A fault tolerant system for current sensors in induction motor drives},
  booktitle = {2015 {{XVI Workshop Inf}}. {{Process}}. {{Control RPIC}}},
  author = {Aguilera, F. and {\noopsort{barrera}}de la Barrera, P. M. and Angelo, C. H. De},
  year = {2015},
  month = oct,
  pages = {1--7},
  doi = {10.1109/RPIC.2015.7497178},
  abstract = {In this work, a fault tolerant control system for current sensors in induction motor drives is proposed. The system is composed by two standar control strategies and a bank of observers. The fault detection and isolation is performed by processing residuals generated by the bank of observers. When sensors are healthy, the motor is controlled by a vector control strategy using two current sensors. If a single current sensor fault occurs, the vector control is maintained by replacing the affected information with estimations generated by the bank of observers. If faults occur in both current sensors, a scalar control strategy is used in order to maintain the operation of the drive. The operation of the proposed system is validated by simulation results.},
  copyright = {All rights reserved},
  file = {/home/facu/Documentos/GEA/papers/2015/Aguilera et al_2015_A fault tolerant system for current sensors in induction motor drives.pdf;/home/facu/documentos/datos/Zotero/storage/3BKQRFXI/articleDetails.html}
}


@inproceedings{aguilera_induction_2012,
  title = {Induction machine models for efficiency studies in {{EV}} design applications},
  booktitle = {2012 10th {{IEEEIAS Int}}. {{Conf}}. {{Ind}}. {{Appl}}. {{INDUSCON}}},
  author = {Aguilera, F. and {\noopsort{barrera}}{de la Barrera}, P.M. and De Angelo, C.H.},
  year = {2012},
  pages = {1--7},
  doi = {10.1109/INDUSCON.2012.6452497},
  abstract = {Different induction machine (IM) models are evaluated, with the aim to determine its usefulness in efficiency studies for electric vehicles (EV) traction drives. In particular, two simple IM models proposed in the literature, which take into account the additional losses, are compared. With this aim, model parameters are adjusted from experimental tests performed on a 4 kW IM. Then, obtained results from both models at different operating points are compared. Even when the estimation errors are similar using both models, one of them is chosen because of its simplicity. Finally, a simple application of this model is proposed, which consists in use the model for obtaining a complete efficiency map of the IM, without need further essays. Such efficiency map can be used to evaluate the overall efficiency of the traction drive of an EV or in EV design.},
  copyright = {All rights reserved},
  file = {/home/facu/documentos/datos/Zotero/storage/RMUK88IJ/Aguilera et al. - 2012 - Induction machine models for efficiency studies in.pdf;/home/facu/GEA/papers/2012/Aguilera et al_2012_Induction machine models for efficiency studies in EV design applications.pdf;/home/facu/documentos/datos/Zotero/storage/CDI55WW8/articleDetails.html}
}


@article{aguilera_speed_2022,
  ids = {aguilera_speed_2022-1},
  title = {Speed and current sensor fault-tolerant induction motor drive for electric vehicles based on virtual sensors},
  author = {Aguilera, Facundo and {\noopsort{barrera}}{de la Barrera}, Pablo Mart{\'i}n and De Angelo, Cristian Hern{\'a}n},
  year = {2022},
  month = mar,
  journal = {Electr Eng},
  issn = {1432-0487},
  doi = {10.1007/s00202-022-01541-4},
  urldate = {2022-07-25},
  abstract = {In this work, a sensor fault-tolerant architecture (FTA) for induction motor drives used in electric vehicles (EV) is presented. A bank of observers is designed based on representations of the induction motor model in different reference frames. The fault diagnosis is performed by processing a set of residuals obtained from the bank of observers. The diagnosis method is able to differentiate between single and multiple faults as well as identify the faulty sensor. In addition, virtual sensors are used for the control feedback reconfiguration to obtain a continuous operation of a vector control strategy after a sensor fault. Simulation results of an EV with the proposed FTA validate the effectiveness of the proposal and its robustness against parameter variations. Moreover, results shows a fast control reconfiguration and the ability to return to normal operation if the sensors recover from the faulty condition.},
  copyright = {All rights reserved},
  langid = {english}
}


