@incollection{chachuat_bounding_2012,
  title = {Bounding the {{Solutions}} of {{Parametric ODEs}}: {{When Taylor Models Meet Differential Inequalities}}},
  shorttitle = {Bounding the {{Solutions}} of {{Parametric ODEs}}},
  booktitle = {Computer {{Aided Chemical Engineering}}},
