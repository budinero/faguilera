@article{fonod_robust_2015,
  title = {Robust {{FDI}} for fault-tolerant thrust allocation with application to spacecraft rendezvous},
  author = {Fonod, Robert and Henry, David and Charbonnel, Catherine and Bornschlegl, Eric and Losa, Damiana and Bennani, Samir},
