  file = {/home/facu/documentos/GEA/papers/2021/Venghi et al_2021_Sistema de control tolerante a fallas en sensores de corriente basado en un.pdf;/home/facu/documentos/datos/Zotero/storage/DCABAW5U/9480151.html;/home/facu/documentos/datos/Zotero/storage/NUDZYNHL/5126.html}
} 

@inproceedings{xacur_instrumento_2016,
  title = {Instrumento para medir dureza de semillas},
  booktitle = {2016 {{IEEE Bienn}}. {{Congr}}. {{Argent}}. {{ARGENCON}}},
  author = {Xacur, M. J. and Aguilera, F. and Airabella, A. M.},
  year = {2016},
  month = jun,
  pages = {1--6},
  doi = {10.1109/ARGENCON.2016.7585351},
  abstract = {This project describes the investigation, development and implementation of an electronic device that measures the required force to break a seed. The device is based on a load cell which measures the force applied to the solid. The device detects the instant when the sample breaks. The obtained hardness values are stored in the device internal memory and are visualized in a liquid crystal display. Also, the device can be connected to a computer to show on a graphic the instantaneous force values applied to a seed or to download the hardness values. The device performance was verified through sample measurements with statistically known hardness values.},
  copyright = {All rights reserved},
  file = {/home/facu/Documentos/GEA/papers/2016/Xacur et al_2016_Instrumento para medir dureza de semillas.pdf;/home/facu/documentos/datos/Zotero/storage/D4NINH42/7585351.html}
} 
