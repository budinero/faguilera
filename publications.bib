@article{son_novel_2018,
  title = {A {{Novel Phase Current Reconstruction Method}} for a {{Three-Level Neutral Point Clamped Inverter}} ({{NPCI}}) with a {{Neutral Shunt Resistor}}},
  author = {Son, Yungdeug and Kim, Jangmok},
  year = {2018},
