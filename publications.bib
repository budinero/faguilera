  year = {2011},
  month = sep,
  journal = {Control Engineering Practice},
  series = {Special {{Section}}: {{DCDS}}'09 {\textendash} {{The}} 2nd {{IFAC Workshop}} on {{Dependable Control}} of {{Discrete Systems}}},
  volume = {19},
