@article{gou_online_2020,
  title = {An {{Online Data-Driven Method}} for {{Simultaneous Diagnosis}} of {{IGBT}} and {{Current Sensor Fault}} of {{Three-Phase PWM Inverter}} in {{Induction Motor Drives}}},
