@article{amezquita-brooks_induction_2015,
  title = {Induction {{Motor Control}}: {{Multivariable Analysis}} and {{Effective Decentralized Control}} of {{Stator Currents}} for {{High-Performance Applications}}},
  shorttitle = {Induction {{Motor Control}}},
  author = {{Am{\'e}zquita-Brooks}, L. A. and {Lic{\'e}aga-Castro}, J. and {Lic{\'e}aga-Castro}, E. and {Ugalde-Loo}, C. E.},
