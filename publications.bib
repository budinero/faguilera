@article{korlinchak_robust_2012,
  title = {Robust {{Sensorless Sliding Mode Flux Observer}} with {{Speed Estimate}} for {{Induction Motor Drives}}},
  author = {Korlinchak, Christopher and Comanescu, Mihai},
