@article{abbasi_post-fault_2022,
  title = {Post-fault operation strategy for single open-phase fault in three-phase induction motor drives},
  author = {Abbasi, Hadi and Ghanbari, Mahmood and Ebrahimi, Reza and Jannati, Mohammad},
  year = {2022},
  journal = {IET Power Electron.},
  volume = {15},
