@inproceedings{satilmis_investigating_2013,
  title = {Investigating {{DC}} link current ripple and {{PWM}} modulation methods in {{Electric Vehicles}}},
  booktitle = {2013 3rd {{Int}}. {{Conf}}. {{Electr}}. {{Power Energy Convers}}. {{Syst}}.},
  author = {Satilmis, Onur and Mese, Erkan},
  year = {2013},
  month = oct,
  pages = {1--6},
