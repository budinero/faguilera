@article{zia_microgrid_2020,
  title = {Microgrid {{Transactive Energy}}: {{Review}}, {{Architectures}}, {{Distributed Ledger Technologies}}, and {{Market Analysis}}},
  shorttitle = {Microgrid {{Transactive Energy}}},
  author = {Zia, Muhammad F. and Benbouzid, Mohamed and Elbouchikhi, Elhoussin and Muyeen, S. M. and Techato, Kuaanan and Guerrero, Josep M.},
