  title = {An {{Accurate Power Control Strategy}} for {{Power-Electronics-Interfaced Distributed Generation Units Operating}} in a {{Low-Voltage Multibus Microgrid}}},
  author = {Li, Yun Wei and Kao, Ching-Nan},
