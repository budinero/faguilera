@article{huang_diagnosis_2018,
  title = {A {{Diagnosis Algorithm}} for {{Multiple Open-Circuited Faults}} of {{Microgrid Inverters Based}} on {{Main Fault Component Analysis}}},
  author = {Huang, Zhanjun and Wang, Zhanshan and Zhang, Huaguang},
  year = {2018},
  month = sep,
  journal = {IEEE Trans. Energy Convers.},
