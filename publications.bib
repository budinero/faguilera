  file = {/home/facu/Documentos/GEA/papers/2007/Barut et al_2007_Speed-Sensorless Estimation for Induction Motors Using Extended Kalman Filters.pdf;/home/facu/documentos/datos/Zotero/storage/KKGCX3JK/articleDetails.html}
