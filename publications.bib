@article{adamczyk_self-correcting_2021,
  title = {Self-{{Correcting Virtual Current Sensor Based}} on the {{Modified Luenberger Observer}} for {{Fault-Tolerant Induction Motor Drive}}},
  author = {Adamczyk, Michal and {Orlowska-Kowalska}, Teresa},
  year = {2021},
