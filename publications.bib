  file = {/home/facu/documentos/GEA/papers/2009/Bodkhe_Aware_2009_Speed-sensorless, adjustable-speed induction motor drive based on dc link.pdf;/home/facu/documentos/datos/Zotero/storage/A3SPFT6T/B84CB3618799.html}
