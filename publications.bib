@article{adamczyk_postfault_2022-1,
  ids = {adamczyk_post-fault_2021,adamczyk_postfault_2022},
  title = {Postfault {{Direct Field-Oriented Control}} of {{Induction Motor Drive Using Adaptive Virtual Current Sensor}}},
