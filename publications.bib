@article{aghenta_design_2019,
  title = {Design and implementation of a low-cost, open source {{IoT-based SCADA}} system using {{ESP32}} with {{OLED}}, {{ThingsBoard}} and {{MQTT}} protocol},
  author = {Aghenta, Lawrence Oriaghe and Iqbal, Tariq},
